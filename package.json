{
	"name": "cgae-siga",
	"version": "1.1.0-SNAPSHOT",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "webpack-dev-server --port=4200",
		"startProxy": "node --max-http-header-size=32768 node_modules/@angular/cli/bin/ng serve --proxy-config src/proxy.conf.json",
		"startProxy2": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --proxy-config src/proxy.conf.json",
		"build": "webpack",
		"deploy": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --env=deloitte --base-href=/siga-ang/",
		"deploytesting": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --env=deloittetesting --base-href=/siga-ang-testing/",
		"deployDesaCGAE": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --env=desacgae --base-href=/htdocs/",
		"deployDesaDebugger": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --env=desacgae --base-href=/htdocs/",
		"cgaeBuild": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --env=cgae --base-href=/htdocs/",
		"cgaeBuildDebugger": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --env=cgae --base-href=/htdocs/",
		"test": "karma start ./karma.conf.js",
		"lint": "ng lint",
		"e2e": "protractor ./protractor.conf.js",
		"pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
		"vars": "env | grep npm_package_",
		"sonar": "sonar-scanner"
	},
	"private": true,
